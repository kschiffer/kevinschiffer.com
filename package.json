  "scripts": {
    "start": "yarn run webpack & node livereload & yarn run server",
    "stop": "killall node",
    "dev": "yarn run server & yarn run nunjucks & yarn run webpack & yarn watch-nunjucks  & yarn run livereload & open http://localhost:8000",
    "webpack": "webpack --watch",
    "livereload": "livereload ./dist",
    "experiments": "http-server ./experiments -p 8000 & livereload ./experiments",
    "nunjucks": "nunjucks *.tpl -O nunjucks.config.json -o dist -p src/markup",
    "watch-nunjucks": "watch 'yarn run nunjucks' ./src/markup --interval=.3",
    "build": "NODE_ENV=production webpack -p",
    "server": "http-server ./dist -p 8000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "async": "^2.6.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jquery": "^3.2.1",
    "jshint": "^2.9.4",
    "jshint-loader": "^0.8.4",
    "node-sass": "^4.5.2",
    "nunjucks-cli": "^0.5.1",
    "nunjucks-watch": "^0.0.14",
    "postcss-csso": "^2.0.0",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "slick-carousel": "^1.8.0",
    "stickybits": "^1.2.8",
    "style-loader": "^0.16.1",
    "watch": "^1.0.2",
    "webpack": "^2.4.1",
    "webpack-notifier": "^1.5.0"
  },
